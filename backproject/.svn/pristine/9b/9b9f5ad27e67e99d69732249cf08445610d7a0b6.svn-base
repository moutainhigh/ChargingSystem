package com.egintra.common.dto;

import com.egintra.common.repository.entity.RightUser;

/**
 * 系统登录成功返回DTO
 *
 * @author liushihao
 * @date 2021/06/24
 */
public class SysInfoDTO {
    /**
     * 用户信息
     */
    private RightUser rightUser;

    /**
     * token
     */
    private String token;

    public RightUser getRightUser() {
        return rightUser;
    }

    public void setRightUser(RightUser rightUser) {
        this.rightUser = rightUser;
    }

    public String getToken() {
        return token;
    }

    public void setToken(String token) {
        this.token = token;
    }

    @Override
    public String toString() {
        return "SysInfoDTO{" +
                "rightUser=" + rightUser +
                ", token='" + token + '\'' +
                '}';
    }
}
