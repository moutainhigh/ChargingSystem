<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.egintra.common.repository.mapper.InvoiceTmpMapper">
    <insert id="addInvoiceTmp" parameterType="com.egintra.common.dto.InvoiceTmp" useGeneratedKeys="true"
            keyProperty="id" keyColumn="id">
        INSERT INTO fee_invoice_tmp(id,
                                    department,
                                    account_id,
                                    invoice_id,
                                    paper_id,
                                    pay_unit,
                                    pay_way,
                                    total_money,
                                    total_pure,
                                    receiver_id,
                                    receiver,
                                    posid,
                                    dates,
                                    dtimes,
                                    receive_account,
                                    check_number,
                                    pos_info,
                                    status,
                                    types,
                                    sfzmhm,
                                    lsh,
                                    cxhp,
                                    ywlx,
                                    ywyy,
                                    ywlb,
                                    orderid)
        values (#{id},
                #{department},
                #{accountId},
                #{invoiceId},
                #{paperId},
                #{payUnit},
                #{payWay},
                #{totalMoney},
                #{totalPure},
                #{receiverId},
                #{receiver},
                #{posId},
                #{dates},
                #{dtimes},
                #{receiveAccount},
                #{checkNumber},
                #{posInfo},
                #{status},
                #{types},
                #{sfzmhm},
                #{lsh},
                #{cxhp},
                #{ywlx},
                #{ywyy},
                #{ywlb},
                #{orderid})
    </insert>

</mapper>