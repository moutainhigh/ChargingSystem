package com.egintra.common.dto.reportForm;

import java.math.BigDecimal;

/**
 * 净收入报表返回DTO
 *
 * @author liushihao
 * @date 2021/11/10
 */
public class NetIncomeReportRespDTO {

    /**
     * 部门（单位）
     */
    private String department;

    /**
     * 项目种类编码
     */
    private String itemCode;

    /**
     * 项目种类
     */
    private String itemName;

    /**
     * 本期收入-省财政
     */
    private BigDecimal curIncomeProFin;

    /**
     * 本期收入-市地财政
     */
    private BigDecimal curIncomeCityFin;

    /**
     * 本期收入-小计
     */
    private BigDecimal curIncomeSubtotal;

    /**
     * 累计收入-省财政
     */
    private BigDecimal cuIncomeProFin;

    /**
     * 累计收入-市地财政
     */
    private BigDecimal cuIncomeCityFin;

    /**
     * 累计收入-小计
     */
    private BigDecimal cuIncomeSubtotal;

    public String getDepartment() {
        return department;
    }

    public void setDepartment(String department) {
        this.department = department;
    }

    public String getItemCode() {
        return itemCode;
    }

    public void setItemCode(String itemCode) {
        this.itemCode = itemCode;
    }

    public String getItemName() {
        return itemName;
    }

    public void setItemName(String itemName) {
        this.itemName = itemName;
    }

    public BigDecimal getCurIncomeProFin() {
        return curIncomeProFin;
    }

    public void setCurIncomeProFin(BigDecimal curIncomeProFin) {
        this.curIncomeProFin = curIncomeProFin;
    }

    public BigDecimal getCurIncomeCityFin() {
        return curIncomeCityFin;
    }

    public void setCurIncomeCityFin(BigDecimal curIncomeCityFin) {
        this.curIncomeCityFin = curIncomeCityFin;
    }

    public BigDecimal getCurIncomeSubtotal() {
        return curIncomeSubtotal;
    }

    public void setCurIncomeSubtotal(BigDecimal curIncomeSubtotal) {
        this.curIncomeSubtotal = curIncomeSubtotal;
    }

    public BigDecimal getCuIncomeProFin() {
        return cuIncomeProFin;
    }

    public void setCuIncomeProFin(BigDecimal cuIncomeProFin) {
        this.cuIncomeProFin = cuIncomeProFin;
    }

    public BigDecimal getCuIncomeCityFin() {
        return cuIncomeCityFin;
    }

    public void setCuIncomeCityFin(BigDecimal cuIncomeCityFin) {
        this.cuIncomeCityFin = cuIncomeCityFin;
    }

    public BigDecimal getCuIncomeSubtotal() {
        return cuIncomeSubtotal;
    }

    public void setCuIncomeSubtotal(BigDecimal cuIncomeSubtotal) {
        this.cuIncomeSubtotal = cuIncomeSubtotal;
    }

    @Override
    public String toString() {
        return "NetIncomeReportRespDTO{" +
                "department='" + department + '\'' +
                ", itemCode='" + itemCode + '\'' +
                ", itemName='" + itemName + '\'' +
                ", curIncomeProFin=" + curIncomeProFin +
                ", curIncomeCityFin=" + curIncomeCityFin +
                ", curIncomeSubtotal=" + curIncomeSubtotal +
                ", cuIncomeProFin=" + cuIncomeProFin +
                ", cuIncomeCityFin=" + cuIncomeCityFin +
                ", cuIncomeSubtotal=" + cuIncomeSubtotal +
                '}';
    }
}
