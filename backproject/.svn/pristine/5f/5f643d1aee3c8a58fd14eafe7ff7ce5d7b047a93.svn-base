package com.egintra.common.dto.reportForm;

import java.math.BigDecimal;

/**
 * 材料消耗明细返回结果对象
 *
 * @author liushihao
 * @date 2021/11/10
 */
public class ConsumeMaterialRespDTO {

    /**
     * 部门（单位）
     */
    private String department;

    /**
     * 项目ID
     */
    private String projectId;

    /**
     * 材料明细号
     */
    private String materialId;

    /**
     * 材料明细名称
     */
    private String materialNm;

    /**
     * 单价
     */
    private BigDecimal materialCost;

    /**
     * 本期数量
     */
    private String curNum;

    /**
     * 本期金额
     */
    private BigDecimal curJe;

    /**
     * 累计数量
     */
    private String cuNum;

    /**
     * 累计金额
     */
    private BigDecimal cuJe;

    public String getDepartment() {
        return department;
    }

    public void setDepartment(String department) {
        this.department = department;
    }

    public String getMaterialId() {
        return materialId;
    }

    public void setMaterialId(String materialId) {
        this.materialId = materialId;
    }

    public String getMaterialNm() {
        return materialNm;
    }

    public void setMaterialNm(String materialNm) {
        this.materialNm = materialNm;
    }

    public BigDecimal getMaterialCost() {
        return materialCost;
    }

    public void setMaterialCost(BigDecimal materialCost) {
        this.materialCost = materialCost;
    }

    public String getCurNum() {
        return curNum;
    }

    public void setCurNum(String curNum) {
        this.curNum = curNum;
    }

    public BigDecimal getCurJe() {
        return curJe;
    }

    public void setCurJe(BigDecimal curJe) {
        this.curJe = curJe;
    }

    public String getCuNum() {
        return cuNum;
    }

    public void setCuNum(String cuNum) {
        this.cuNum = cuNum;
    }

    public BigDecimal getCuJe() {
        return cuJe;
    }

    public void setCuJe(BigDecimal cuJe) {
        this.cuJe = cuJe;
    }

    public String getProjectId() {
        return projectId;
    }

    public void setProjectId(String projectId) {
        this.projectId = projectId;
    }

    @Override
    public String toString() {
        return "ConsumeMaterialRespDTO{" +
                "department='" + department + '\'' +
                ", projectId='" + projectId + '\'' +
                ", materialId='" + materialId + '\'' +
                ", materialNm='" + materialNm + '\'' +
                ", materialCost=" + materialCost +
                ", curNum='" + curNum + '\'' +
                ", curJe=" + curJe +
                ", cuNum='" + cuNum + '\'' +
                ", cuJe=" + cuJe +
                '}';
    }
}
